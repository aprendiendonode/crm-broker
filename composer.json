{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "astrotomic/laravel-translatable": "^11.9",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "bmatovu/laravel-xml": "^3.0",
        "dirape/token": "^2.3",
        "doctrine/dbal": "^2.12.1",
        "enlightn/enlightn": "^1.22",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.5",
        "joedixon/laravel-translation": "^1.1",
        "laravel-notification-channels/facebook": "^0.2.1",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravelium/feed": "7.*",
        "maatwebsite/excel": "^3.1",
        "mike42/escpos-php": "^3.0",
        "nwidart/laravel-modules": "^8.2",
        "patrickschur/language-detection": "^5.1",
        "pusher/pusher-php-server": "^4.0",
        "renatomarinho/laravel-page-speed": "^2.0",
        "spatie/laravel-feed": "^2.2",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^4.0",
        "spatie/laravel-ray": "^1.13", 
        "spatie/laravel-model-states": "^1.6",
        "spatie/laravel-query-builder": "^3.0",
        "spatie/laravel-view-models": "^1.3",
        "spatie/period": "^1.5",
        "spatie/query-string": "^1.0"
   
   
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "imanghafoori/laravel-microscope": "^1.0",
        "laravel/telescope": "^3.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.7.12",
        "orchestra/testbench": "~5.0",
        "phpunit/phpunit": "^8.5",
        "barryvdh/laravel-ide-helper": "2.6.*",
        "composer/composer": "^1.10@dev"
     
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "platform-check": false,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Domain\\": "src/Domain",
            "Support\\": "src/Support"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
